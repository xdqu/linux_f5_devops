            state up
        }
        apigateway-tisdev:https {
            address 10.60.16.44
            monitor min 1 of { monitor_tcp_apigateway-tisdev_https }
            priority-group 2
            session monitor-enabled
            state up
        }
    }
    min-active-members 2
}
