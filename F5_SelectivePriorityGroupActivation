ltm monitor tcp monitor_tcp_apigateway-tisdev_https {
    adaptive disabled
    defaults-from tcp
    destination 10.60.16.44:https
    interval 5
    ip-dscp 0
    recv none
    recv-disable none
    send none
    time-until-up 0
    timeout 16
}


